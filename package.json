{
  "name": "prettier-plugin-pg",
  "version": "0.0.0",
  "description": "A prettier plugin for formatting PostgreSQL-flavour SQL",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "jest",
    "prettier": "tsc && prettier --plugin=. --parser=postgresql",
    "add-statement": "./scripts/add_statement.js"
  },
  "repository": {
    "type": "git",
    "url": "https+git://github.com/benjie/prettier-plugin-pg.git"
  },
  "keywords": [
    "prettier",
    "sql",
    "pgsql",
    "postgres",
    "postgresql",
    "plpgsql",
    "plv8",
    "psql"
  ],
  "author": "Benjie Gillam <code@benjiegillam.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "12.x",
    "babel-eslint": "10.1.0",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-import": "2.20.2",
    "eslint-plugin-jest": "23.8.2",
    "eslint-plugin-prettier": "3.1.3",
    "jest": "25.4.0",
    "jest-runner-eslint": "0.7.7",
    "prettier": "2.0.5",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "pg-query-native": "1.2.0"
  },
  "files": [
    "index.js",
    "embed.js",
    "print.js",
    "reservedWords.js"
  ],
  "jest": {
    "projects": [
      "<rootDir>/jest.*.config.js"
    ]
  }
}
